Dim TestAddress As Word
Dim InData As Word
Dim ExpectedValue As Word

DebugPrint "Data table test"
DebugPrintCRLF
Wait 10 ms

Results = 0
For TestAddress = 1 to 500
	ReadTable BigTable, TestAddress, InData
	If InData <> TestAddress * 2 Then
		Set Results.0 On
	End If
Next

ErrorFound = FALSE
For TestAddress = 1 to 10
	ReadTable SmallerTable, TestAddress, InByte
	If InByte <> TestAddress * 3 Then
		Set Results.1 On
	End If
Next

ExpectedValue = 1024
For TestAddress = 1 to 11
	ReadTable TrimmableTable, TestAddress, InData
	If InData <> ExpectedValue Then
		Set Results.2 On
	End If
	ExpectedValue = ExpectedValue / 2
Next

ReadTable BigByteTable, 0, InData
If InData <> 511 Then
	Set Results.3 On
End If
For TestAddress = 1 to 511
	ReadTable BigByteTable, TestAddress, InByte
	If InByte <> [byte]TestAddress Then
		Set Results.4 On
	End If
Next

'Test read beyond end
TestAddress = 10
ReadTable SmallerTable, TestAddress, InByte
If InByte <> 30 Then
	Set Results.5 On
End If
TestAddress = 11
ReadTable SmallerTable, TestAddress, InByte
If InByte <> 0 Then
	Set Results.6 On
End If

ReadTable BigTable, 0, TestAddress
ReadTable BigTable, TestAddress + 1, InData
If InData <> 0 Then
	Set Results.7 On
End If

DebugPrint "Expected 0x00, result 0x"
DebugPrint Hex(Results)
DebugPrintCRLF

Table SmallerTable
3,6,9,12,15
18,21,24,27,30
End Table

